{
    "version": 2,
    "builds":[
        { 
          "src": "server.js", 
          "use": "@vercel/node" 
       }
        
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"server.js"
          
        },

         { "src": "/user","methods": ["POST"], "dest": "/(.*)"  },
    { "src": "/products/(?<limit>[^/]+)",  "dest": "products?limit=9&&&title[regex]="}
    ]
}

